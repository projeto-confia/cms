{
  "kind": "singleType",
  "collectionName": "contact_pages",
  "info": {
    "name": "Contact Page"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "commonPageData": {
      "type": "component",
      "repeatable": false,
      "component": "page.common-page-data"
    },
    "title": {
      "type": "string",
      "required": true,
      "default": "Contatos"
    },
    "socialNetworkLinks": {
      "type": "component",
      "repeatable": true,
      "component": "page.social-network-link"
    },
    "email": {
      "type": "string"
    }
  }
}
